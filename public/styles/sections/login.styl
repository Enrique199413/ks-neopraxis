.login-header
  padding-top 96px
  background-color #6d989c;
  height 678px
  justify-content flex-start
.login-title-box
  padding-top 79px
.login-title
  font-size 28px
.letter-spacing-dot-two
  letter-spacing 0.2px
.login-form-box
  padding-top 80px
.login-input-box
  width 284px
  padding-bottom 12px
.login-input
  width 100%
  border-radius 3px
  border solid 1px #e5e5e5
  background-color #FFFFFF
  color #9b9b9b
  font-size 11.5px
  height 35px
  padding-left 16px
.login-input::placeholder
  color #9b9b9b
.login-send-btn
  border none
  outline none
  background-color #6b806c
  font-size 11px
  padding 9px 15px
  border-radius 20px
  letter-spacing 0.2px
  margin-top 40px